# hdevieux
# .bash_aliases
# This file is managed by dotfiles repository
# Last updated: 6/04/2018

# shell aliases
alias c="clear"

# tmux aliases
alias tmux="TERM=screen-256color-bce tmux"
alias tn="tmux_rename"
alias tm="tmux attach -t main || tmux new -s main"

# git aliases
alias g="git"
alias gp="git pull"
alias gs="git status"
alias gd="git diff"
